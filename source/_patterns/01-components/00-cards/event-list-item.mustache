
<div class="card event-card-list w-100">
  <div class="row">
    <div class="col-4 col-md-3 pl-0 pr-0 py-3">
      &nbsp;
    </div>
    <div class="col-6 col-md-3 m-0">
      <div class="card-block my-4 p-0">
        <h3 class="card-title display5"><strong>{{ cards.eventTitle }}</strong></h3>
      </div>
    </div>
    <div class="col-1 hidden-sm-down">&nbsp;</div>
    <div class="col-2 my-4 p-0 hidden-sm-down">
      <p class="display7 secondary mt-1 mb-0">{{ cards.eventDate }}</p>
      <p class="display7 secondary m-0">{{ cards.eventLocale }}</p>
      <p class="display7 secondary m-0">{{ cards.eventCity }}</p>
    </div>
  </div>
  <div class="art-list-item-icons hidden-sm-down">
    <img class="mr-5" src="{{ cards.shareImg }}" alt="Share Icon">
    <img class="mr-3" src="{{ cards.saveImg }}" alt="Favorite Icon">
  </div>
</div>
