
<div class="card member-card-list w-100">
  <div class="row">
    <div class="col-4 col-md-3 d-flex align-items-center">
      <img class="member-card-img mx-auto" src="{{ cards.memberCardImg }}" alt="{{ cards.memberName }}">
    </div>
    <div class="col-8 col-sm-6 col-md-4 m-0">
      <div class="card-block my-4 p-0">
        <h3 class="card-title display6"><strong>{{ cards.memberName }}</strong></h3>
        <p class="display8 secondary mt-1 mb-0">{{ cards.memberTitle }}</p>
        <p class="display8 accent mt-0 mb-0">{{ cards.memberCompany }}</p>
      </div>
    </div>
    <div class="col-1 hidden-sm-down">&nbsp;</div>
    <div class="col-2 my-4 p-0 hidden-sm-down">
      <p class="display8 secondary mt-1 mb-0">{{ cards.memberLocale }}</p>
      <p class="display8 secondary m-0">{{ cards.memberCountry }}</p>
    </div>
  </div>
  <div class="gen-list-item-icons hidden-sm-down">
    <img class="mr-5" src="{{ cards.shareImg }}" alt="Share Icon">
    <img class="mr-3" src="{{ cards.saveImg }}" alt="Favorite Icon">
  </div>
</div>
