
<div class="col-md-4 publication-card">
  <div class="card">
    <img class="publication-card-img mt-4 mx-auto" src="{{ pubCardImg }}" alt="{{ pubCardAlt }}">
    <div class="card-block">
      <h3 class="card-title mt-3 display5">{{ pubTitle }}</h3>
      <p class="display7 secondary mt-3 mb-0">{{ pubCat }}</p>
    </div>
    <div class="card-icons mt-3 mt-md-5 p-3 w-100 d-flex justify-content-end">
      <img class="mr-5" src="{{ shareImg }}" alt="Share Icon">
      <img class="mr-3" src="{{ saveImg }}" alt="Favorite Icon">
    </div>
  </div>
</div>
